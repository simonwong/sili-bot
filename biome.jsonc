{
  "$schema": "./node_modules/@biomejs/biome/configuration_schema.json",
  "extends": [
    "ultracite"
  ],
  "linter": {
    "rules": {
      "nursery": {
        "useNumericSeparators": "off",
        "noUnknownAtRule": "off"
      },
      "performance": {
        "useTopLevelRegex": "off"
      },
      "style": {
        "noNonNullAssertion": "off"
      },
      "suspicious": {
        "useAwait": "off"
      }
    }
  }
}