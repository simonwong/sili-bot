{
  "$schema": "https://biomejs.dev/schemas/2.1.2/schema.json",
  "extends": ["ultracite"],
  "linter": {
    "rules": {
      "nursery": {
        "useNumericSeparators": "off",
        "noUnknownAtRule": "off"
      },
      "performance": {
        "useTopLevelRegex": "off"
      },
      "style": {
        "noNonNullAssertion": "off"
      },
      "suspicious": {
        "useAwait": "off"
      }
    }
  }
}
